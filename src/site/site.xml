<?xml version="1.0" encoding="UTF-8"?>
<project name="Properties Constants Maven Plugin">
    <bannerLeft>
        <name>Properties Constants Maven Plugin</name>
    </bannerLeft>
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.7</version>
    </skin>
